{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destination"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "blocks"
    },
    "Region": {
      "type": "string"
    },
    "State": {
      "type": "enumeration",
      "enum": [
        "Andaman and Nicobar Islands",
        "Andhra Pradesh",
        "Arunachal Pradesh",
        "Assam",
        "Bihar",
        "Chhattisgarh",
        "Dadra and Nagar Haveli and Daman and Diu",
        "Delhi",
        "Goa",
        "Gujarat",
        "Haryana",
        "Himachal Pradesh",
        "Jharkhand",
        "Karnataka",
        "Kerala",
        "Lakshadweep",
        "Madhya Pradesh",
        "Maharashtra",
        "Manipur",
        "Meghalaya",
        "Mizoram",
        "Nagaland",
        "Odisha",
        "Puducherry",
        "Punjab",
        "Rajasthan",
        "Sikkim",
        "Tamil Nadu",
        "Telangana",
        "Tripura",
        "Chandigarh",
        "Jammu and Kashmir",
        "Ladakh",
        "Uttar Pradesh",
        "Uttarakhand",
        "West Bengal"
      ]
    },
    "Country": {
      "type": "string",
      "default": "INDIA"
    },
    "Lattitude": {
      "type": "float",
      "required": true
    },
    "Longitude": {
      "type": "float",
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "destinations"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "attractions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::attraction.attraction",
      "mappedBy": "destinations"
    },
    "BestSeason": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Winter (DEC-FEB)",
        "Summer (APR-JUNE)",
        "Monsoon (JUNE - SEP)",
        "Autumn (OCT-DEC)",
        "Spring: (FEB-MARCH)"
      ],
      "min": 1
    }
  }
}
