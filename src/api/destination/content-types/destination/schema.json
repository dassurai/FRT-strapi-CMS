{
  "kind": "collectionType",
  "collectionName": "destinations",
  "info": {
    "singularName": "destination",
    "pluralName": "destinations",
    "displayName": "Destination"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "BestSeason": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Winter (Dec-Feb)",
        "Summer (Apr-June)",
        "Monsoon (Juneâ€“Sep)",
        "Autumn (Oct-Dec)",
        "Spring (Feb-Mar)"
      ],
      "min": 1
    },
    "Description": {
      "type": "blocks"
    },
    "location": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker"
    },
    "Media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "terrain": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Mountain",
        "Valley",
        "Hill",
        "Plateau",
        "River-side",
        "Beach",
        "Lake",
        "Waterbodies",
        "Forest",
        "Desert",
        "Farm-land",
        "Canyon"
      ]
    },
    "Region": {
      "type": "string"
    },
    "District": {
      "type": "string"
    },
    "State": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Andaman and Nicobar Islands",
        "Andhra Pradesh",
        "Arunachal Pradesh",
        "Assam",
        "Bihar",
        "Chhattisgarh",
        "Dadra and Nagar Haveli and Daman and Diu",
        "Delhi",
        "Goa",
        "Gujarat",
        "Haryana",
        "Himachal Pradesh",
        "Jharkhand",
        "Karnataka",
        "Kerala",
        "Lakshadweep",
        "Madhya Pradesh",
        "Maharashtra",
        "Manipur",
        "Meghalaya",
        "Mizoram",
        "Nagaland",
        "Odisha",
        "Puducherry",
        "Punjab",
        "Rajasthan",
        "Sikkim",
        "Tamil Nadu",
        "Telangana",
        "Tripura",
        "Chandigarh",
        "Jammu and Kashmir",
        "Ladakh",
        "Uttar Pradesh",
        "Uttarakhand",
        "West Bengal"
      ]
    },
    "Country": {
      "type": "string",
      "default": "INDIA",
      "required": true
    },
    "station": {
      "type": "component",
      "component": "shared.transport",
      "repeatable": false
    },
    "airport": {
      "type": "component",
      "component": "shared.transport",
      "repeatable": false
    },
    "busStand": {
      "type": "component",
      "component": "shared.transport",
      "repeatable": false
    },
    "attractions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::attraction.attraction",
      "mappedBy": "destinations"
    },
    "destination_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::place-tag.place-tag",
      "inversedBy": "destinations"
    }
  }
}
